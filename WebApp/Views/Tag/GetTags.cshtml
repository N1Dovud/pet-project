@model List<TagModel?>?;

<h1>Tags</h1>
<div class="tag-list">
    @foreach (var tag in @Model ?? []) {
        @await Html.PartialAsync("Partials/Tag/_TaskTagCard", new TagViewModel
        {
            Tag = tag,
            FiltersTasks = true,
            HasDeleteButton = false,
        })
    }
</div>